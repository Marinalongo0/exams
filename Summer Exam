import random 

randoNum = 35
guess = random.randint(1,50)

usernum = int(input("Guess a number from 1 to 50: "))
if (guess < randoNum):
     print("Your guess is too high! A bit lower.")
elif (guess > randoNum):
    print("Your guess is too low. Higher!")
